{
  "rules": {
    ".read": true,
    ".write": false,
    "days_exercised": {
      "$user": {
        ".indexOn": "timeStamp"
      }
    },
    "exercises": {
      "$user": {
        ".indexOn": "timeStamp"
      }
    },
    "new_exercises": {
      "$user": {
        ".write": "$user === auth.token.email",
        ".indexOn": "timeStamp"
      }
    }
  }
}
